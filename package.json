    "url": "http://gajus.com"
  },
  "dependencies": {
    "@typescript-eslint/utils": "^8.18.1",
    "astring": "^1.9.0",
    "debug": "^4.4.0",
    "lodash": "^4.17.21",
    "pg-formatter": "^2.0.6",
    "sql-parse": "^0.1.5"
  },
  "description": "SQL linting rules for ESLint.",
  "devDependencies": {
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/github": "^11.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.2",
    "@typescript-eslint/rule-tester": "^8.18.1",
    "cspell": "^8.17.1",
    "eslint": "^9.17.0",
    "eslint-config-canonical": "44.3.35",
    "gitdown": "^4.1.1",
    "glob": "^11.0.0",
    "husky": "^9.1.7",
    "mocha": "^11.0.1",
    "semantic-release": "^24.2.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=12"
  },
  "files": [
    "src",
    "dist"
  ],
  "keywords": [
    "eslint",
    "plugin",
    "sql"
  ],
  "license": "BSD-3-Clause",
  "main": "./dist/index.js",
  "name": "eslint-plugin-sql",
  "peerDependencies": {
    "eslint": ">=8.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gajus/eslint-plugin-sql"
  },
  "scripts": {
    "build": "npm run documentation && tsc",
    "documentation": "gitdown ./.README/README.md --output-file ./README.md && npm run documentation-add-assertions",
    "documentation-add-assertions": "tsx ./bin/readmeAssertions",
    "lint:cspell": "cspell './**/*.{ts,tsx}' --no-progress --gitignore",
    "lint:eslint": "eslint ./src ./test",
    "lint:tsc": "tsc --noEmit",
    "prepare": "husky install",
    "test:mocha": "mocha --require tsx test/**/*"
  },
  "types": "./dist/index.d.ts",
  "version": "1.0.1"
}