  "dependencies": {
    "@typescript-eslint/utils": "^8.31.0",
    "debug": "^4.4.0",
    "lodash": "^4.17.21",
    "multiline-ts": "^4.0.1",
    "sql-formatter": "^15.6.1",
    "sql-parse": "^0.1.5"
  },
  "description": "SQL linting rules for ESLint.",
  "devDependencies": {
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/github": "^11.0.2",
    "@semantic-release/npm": "^12.0.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.15.2",
    "@typescript-eslint/rule-tester": "^8.31.0",
    "cspell": "^8.19.2",
    "eslint": "^9.25.1",
    "eslint-config-canonical": "44.9.5",
    "gitdown": "^4.1.1",
    "glob": "^11.0.2",
    "husky": "^9.1.7",
    "mocha": "^11.1.0",
    "semantic-release": "^24.2.3",
    "tsup": "^8.5.1",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.0"
  },
  "engines": {
    "node": ">=12"
  },
  "exports": {
    "types": "./dist/index.d.ts",
    "require": "./dist/index.cjs",
    "import": "./dist/index.js",
    "default": "./dist/index.js"
  },
  "files": [
    "src",
    "dist"
  ],
  "keywords": [
    "eslint",
    "plugin",
    "sql"
  ],
  "license": "BSD-3-Clause",
  "main": "./dist/index.cjs",
  "name": "eslint-plugin-sql",
  "peerDependencies": {
    "eslint": ">=8.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gajus/eslint-plugin-sql"
  },
  "scripts": {
    "build": "tsup",
    "documentation-add-assertions": "tsx ./src/bin/readmeAssertions",
    "generate": "gitdown ./.README/README.md --output-file ./README.md && npm run documentation-add-assertions",
    "lint:cspell": "cspell './**/*.{ts,tsx}' --no-progress --gitignore",
    "lint:eslint": "eslint ./src",
    "lint:tsc": "tsc --noEmit",
    "prepare": "husky",
    "test": "npm run test:mocha",
    "test:mocha": "mocha --require tsx \"{src,test}/**/*.{test.ts,test.cjs}\""
  },
  "type": "module",
  "types": "./dist/index.d.ts",
  "version": "1.0.1"
}