    "@babel/plugin-transform-flow-strip-types": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "@babel/register": "^7.11.5",
    "babel-plugin-add-module-exports": "^1.0.4",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-canonical": "24.1.1",
    "gitdown": "^3.1.3",
    "glob": "^7.1.6",
    "husky": "^4.3.0",
    "jsonlint": "^1.6.3",
    "mocha": "^8.1.3",
    "semantic-release": "^17.1.1"
  },
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "eslint",
    "plugin",
    "sql"
